[request]
POST "https://jsonplaceholder.typicode.com/posts"

[headers]
Authorization: Bearer exampletoken12345
Content-Type: application/json
Custom-Header: custom_value

[body type="json"]
{
  "title": "foo",
  "body": "bar",
  "userId": 1
}

[assertion on="status"]
201

[assertion on="response"]
{
  "message": "foo"
}
